{
  "name": "web",
  "version": "3.0.0",
  "description": "Request Yo Racks",
  "main": "index.js",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "ci": "run-s lint:* test",
    "export": "next export",
    "fix": "run-s fix:*",
    "fix:format": "prettier --write 'pages/**/*.js' 'components/**/*.js'",
    "fix:javascript": "eslint --ext js,html --fix pages components",
    "lint": "run-s lint:*",
    "lint:format": "prettier -l --debug-check 'pages/**/*.js' 'components/**/*.js'",
    "lint:javascript": "eslint --ext js,html pages components",
    "start": "NODE_ENV=production node server.js",
    "test": "NODE_ENV=test jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/request-yo-racks/landing-page.git"
  },
  "keywords": [
    "bike",
    "racks",
    "parking",
    "ryr"
  ],
  "author": "The RYR team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/request-yo-racks/landing-page/issues"
  },
  "homepage": "https://github.com/request-yo-racks/landing-page#readme",
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "express": "^4.16.4",
    "next": "^7.0.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^23.6.0",
    "jest-environment-enzyme": "^7.0.0",
    "jest-enzyme": "^7.0.0",
    "next-workbox-webpack-plugin": "^1.1.0",
    "npm-run-all": "^4.1.3",
    "prettier": "1.14.3",
    "prettier-eslint": "^8.8.2",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.6.0",
    "webpack-pwa-manifest": "^3.7.1"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ]
      },
      "test": {
        "presets": [
          "env",
          "react"
        ]
      }
    }
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parser": "babel-eslint",
    "settings": {
      "react": {
        "createClass": "createReactClass",
        "pragma": "React",
        "version": "16.0"
      },
      "propWrapperFunctions": [
        "forbidExtraProps"
      ]
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-console": "off",
      "react/react-in-jsx-scope": "off"
    },
    "parserOptions": {
      "ecmaVersion": 2015,
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    }
  },
  "jest": {
    "setupTestFrameworkScriptFile": "jest-enzyme",
    "testEnvironment": "enzyme",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none"
  }
}
